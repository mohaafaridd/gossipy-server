{
  "name": "gossipy-server",
  "author": "Mohammed Farid",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "heroku-postbuild": "prisma generate && npm run build",
    "build": "tsc --p tsconfig.prod.json",
    "start": "env-cmd -f ./config/prod.env node ./dist/index.js",
    "dev": "env-cmd -f ./config/prod.env ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "test": "env-cmd -f ./config/test.env jest --watch --runInBand",
    "seed": "env-cmd -f ./config/test.env jest seed.test.ts --watch --runInBand"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@prisma/cli": "^2.0.0-beta.2",
    "jest": "^25.2.7",
    "ts-jest": "^25.3.1",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-beta.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/module-alias": "^2.0.0",
    "@types/sharp": "^0.24.0",
    "@types/validator": "^13.0.0",
    "@types/ws": "^7.2.3",
    "apollo-server": "^2.12.0",
    "aws-sdk": "^2.657.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^3.0.0",
    "crypto": "^1.0.1",
    "env-cmd": "^10.0.1",
    "graphql": "^14.6.0",
    "graphql-import": "^1.0.2",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "moment": "^2.24.0",
    "sharp": "^0.25.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^3.8.3",
    "validator": "^13.0.0"
  },
  "_moduleAliases": {
    "@src": "./dist/*",
    "@utils": "./dist/utils/index.js",
    "@constants": "./dist/constants/index.js",
    "@mutations/*": "./dist/resolvers/Mutation/*",
    "@queries/*": "./dist/resolvers/Query/*"
  }
}
